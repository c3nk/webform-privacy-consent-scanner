{
  "name": "webform-privacy-consent-scanner",
  "version": "0.1.7",
  "description": "Advanced web form scanner detecting Google Forms, HubSpot Forms, Microsoft Forms, Formstack Forms with comprehensive CMP detection including Cookiebot, OneTrust, Efilli, and GDPR compliance auditing. Also detects 3rd party application links and embedded forms on websites.",
  "main": "scanner.mjs",
  "type": "module",
  "bin": {
    "webform-scanner": "scanner.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c3nk/webform-privacy-consent-scanner.git"
  },
  "homepage": "https://github.com/c3nk/webform-privacy-consent-scanner#readme",
  "bugs": {
    "url": "https://github.com/c3nk/webform-privacy-consent-scanner/issues"
  },
  "keywords": [
    "privacy",
    "gdpr",
    "cmp",
    "cookiebot",
    "onetrust",
    "efilli",
    "hubspot",
    "google-forms",
    "microsoft-forms",
    "webform",
    "scanner",
    "node",
    "cli",
    "playwright"
  ],
  "author": "c3nk.com <me@c3nk.com>",
  "license": "MIT",
  "files": [
    "scanner.mjs",
    "filter.mjs",
    "lib/",
    "form-detection-config.json",
    "bin/",
    "README.md",
    "USERGUIDE.md",
    "USERGUIDE_EN.md",
    "CHANGELOG.md",
    "LICENSE",
    "examples/",
    "docs/social-preview.png"
  ],
  "scripts": {
    "start": "node scanner.mjs --help",
    "scan:static": "node scanner.mjs --input examples/urls.sample.txt --out examples/results.sample.csv --cmp",
    "scan:full": "node scanner.mjs --input examples/urls.sample.txt --out examples/results.sample.csv --cmp",
    "filter": "node filter.mjs --input results.json --attr is_google_form --value true",
    "prepare": "node -e \"console.log('Ready')\""
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "p-limit": "^7.1.1"
  },
  "devDependencies": {
    "eslint": "^9.11.0",
    "eslint-plugin-import": "^2.29.1",
    "playwright": "^1.55.0"
  },
  "engines": {
    "node": ">=18"
  }
}
