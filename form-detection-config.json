{
  "version": "1.0.0",
  "forms": {
    "yourcompany": {
      "name": "Your Company Forms",
      "enabled": true,
      "patterns": [
        {
          "type": "url",
          "pattern": "https?:\\/\\/forms\\.yourcompany\\.com\\/[a-z0-9_-]+",
          "description": "Your Company hosted form URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/yourcompany\\.com\\/contact\\/form\\/[a-z0-9_-]+",
          "description": "Your Company contact form URL"
        },
        {
          "type": "html",
          "pattern": "<iframe[^>]+src=[\"']https?:\\/\\/forms\\.yourcompany\\.com\\/[^\"']+[\"']",
          "description": "Your Company form iframe embed"
        },
        {
          "type": "html",
          "pattern": "<form[^>]+action=[\"']https?:\\/\\/forms\\.yourcompany\\.com\\/[^\"']+[\"']",
          "description": "Your Company form action"
        }
      ]
    },
    "ozyegin": {
      "name": "Özyeğin University Forms",
      "enabled": true,
      "patterns": [
        {
          "type": "url",
          "pattern": "https?:\\/\\/form\\.ozyegin\\.edu\\.tr\\/[a-z0-9_-]*",
          "description": "Özyeğin University form URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/form\\.ozyegin\\.edu\\.tr\\/.*",
          "description": "Özyeğin University form with path"
        },
        {
          "type": "html",
          "pattern": "<iframe[^>]+src=[\"']https?:\\/\\/form\\.ozyegin\\.edu\\.tr\\/[^\"']*[\"']",
          "description": "Özyeğin University form iframe embed"
        },
        {
          "type": "html",
          "pattern": "<form[^>]+action=[\"']https?:\\/\\/form\\.ozyegin\\.edu\\.tr\\/[^\"']*[\"']",
          "description": "Özyeğin University form action"
        }
      ]
    },
    "example": {
      "name": "Example Custom Form",
      "enabled": false,
      "patterns": [
        {
          "type": "url",
          "pattern": "https?:\\/\\/forms\\.example\\.com\\/[a-z0-9_-]+",
          "description": "Example custom form URL"
        },
        {
          "type": "html",
          "pattern": "example-form-[a-z0-9_-]+",
          "description": "Example form ID in HTML"
        }
      ]
    },
    "google": {
      "name": "Google Forms",
      "enabled": true,
      "patterns": [
        {
          "type": "url",
          "pattern": "https?:\\/\\/docs\\.google\\.com\\/forms\\/d\\/e\\/[A-Za-z0-9_-]+\\/(?:viewform|formResponse)",
          "description": "Google Forms direct URL pattern"
        },
        {
          "type": "html",
          "pattern": "<iframe[^>]+src=[\"']https?:\\/\\/docs\\.google\\.com\\/forms\\/[^\"']+[\"']",
          "description": "Google Forms iframe embed"
        },
        {
          "type": "html",
          "pattern": "<form[^>]+action=[\"']https?:\\/\\/docs\\.google\\.com\\/forms\\/[^\"']+[\"']",
          "description": "Google Forms form action"
        },
        {
          "type": "html",
          "pattern": "docs\\.google\\.com\\/forms",
          "description": "General Google Forms reference"
        }
      ]
    },
    "hubspot": {
      "name": "HubSpot Forms",
      "enabled": true,
      "patterns": [
        {
          "type": "script",
          "pattern": "https?:\\/\\/js\\.hsforms\\.net\\/forms\\/(?:embed\\/)?v2\\.js",
          "description": "HubSpot forms script"
        },
        {
          "type": "javascript",
          "pattern": "hbspt\\.forms\\.create\\s*\\(",
          "description": "HubSpot forms.create function call"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/api\\.hsforms\\.com\\/submissions\\/v3\\/integration\\/submit\\/\\d+\\/[0-9a-f-]+",
          "description": "HubSpot API submission endpoint"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/forms\\.hubspot\\.com\\/uploads\\/form\\/v2\\/\\d+\\/[0-9a-f-]+",
          "description": "HubSpot form upload endpoint"
        },
        {
          "type": "html",
          "pattern": "<div[^>]+id=[\"']hubspotForm[\"'][^>]*>",
          "description": "HubSpot form container"
        },
        {
          "type": "html",
          "pattern": "data-hs-captcha",
          "description": "HubSpot captcha element"
        },
        {
          "type": "javascript",
          "pattern": "portalId[\"']?\\s*:\\s*\\d+",
          "description": "HubSpot portal ID"
        },
        {
          "type": "html",
          "pattern": "hs-script-[a-f0-9]+",
          "description": "HubSpot script ID"
        }
      ]
    },
    "formstack": {
      "name": "Formstack Forms",
      "enabled": true,
      "patterns": [
        {
          "type": "url",
          "pattern": "https?:\\/\\/[a-z0-9-]+\\.formstack\\.com\\/forms\\/[a-z0-9_-]+",
          "description": "Formstack hosted form URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/formstack\\.com\\/forms\\/[a-z0-9_-]+",
          "description": "Formstack direct form URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/[a-z0-9-]+\\.formstack\\.com\\/forms\\/[a-z0-9_-]+\\/view",
          "description": "Formstack form view URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/[a-z0-9-]+\\.formstack\\.com\\/forms\\/[a-z0-9_-]+\\/embed",
          "description": "Formstack embedded form URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/formstack\\.com\\/forms\\/[a-z0-9_-]+\\/preview",
          "description": "Formstack form preview URL"
        },
        {
          "type": "script",
          "pattern": "https?:\\/\\/[a-z0-9-]+\\.formstack\\.com\\/forms\\/js\\.php",
          "description": "Formstack embed script"
        },
        {
          "type": "script",
          "pattern": "https?:\\/\\/static\\.formstack\\.com\\/forms\\/js\\/[a-z0-9_-]+\\.js",
          "description": "Formstack static embed script"
        },
        {
          "type": "html",
          "pattern": "<iframe[^>]+src=[\"']https?:\\/\\/[a-z0-9-]+\\.formstack\\.com\\/forms\\/[^\"']+[\"']",
          "description": "Formstack iframe embed"
        },
        {
          "type": "html",
          "pattern": "<script[^>]+src=[\"']https?:\\/\\/[a-z0-9-]+\\.formstack\\.com\\/forms\\/js\\.php[^\"']+[\"']",
          "description": "Formstack embed script tag"
        },
        {
          "type": "javascript",
          "pattern": "Formstack\\.init\\s*\\(",
          "description": "Formstack JavaScript initialization"
        },
        {
          "type": "javascript",
          "pattern": "fsForm\\d+",
          "description": "Formstack form ID variable"
        },
        {
          "type": "html",
          "pattern": "data-formstack-[a-z]+",
          "description": "Formstack data attributes"
        },
        {
          "type": "html",
          "pattern": "class=[\"'][^\"']*formstack[^\"']*[\"']",
          "description": "Formstack CSS classes"
        }
      ]
    },
    "microsoft": {
      "name": "Microsoft Forms",
      "enabled": true,
      "patterns": [
        {
          "type": "url",
          "pattern": "https?:\\/\\/forms\\.office\\.com\\/Pages\\/ResponsePage\\.aspx\\?id=[^&\\/]+",
          "description": "Microsoft Forms response page URL"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/forms\\.office\\.com\\/r\\/[A-Za-z0-9_-]+",
          "description": "Microsoft Forms short response URL"
        },
        {
          "type": "html",
          "pattern": "<iframe[^>]+src=[\"']https?:\\/\\/forms\\.office\\.com\\/[^\"']+[\"']",
          "description": "Microsoft Forms iframe embed"
        },
        {
          "type": "javascript",
          "pattern": "forms\\.office\\.com",
          "description": "Microsoft Forms domain reference"
        },
        {
          "type": "html",
          "pattern": "data-form-id=[\"'][A-Za-z0-9_-]+[\"']",
          "description": "Microsoft Forms data attribute"
        },
        {
          "type": "html",
          "pattern": "class=[\"'][^\"']*office-form[^\"']*[\"']",
          "description": "Microsoft Forms CSS class"
        },
        {
          "type": "javascript",
          "pattern": "OfficeFabric",
          "description": "Microsoft Office UI framework"
        },
        {
          "type": "url",
          "pattern": "https?:\\/\\/forms\\.microsoft\\.com\\/[A-Za-z0-9_-]+",
          "description": "Microsoft Forms domain"
        }
      ]
    }
  },
  "cmp": {
    "name": "Cookie Consent Management Platform",
    "enabled": true,
    "patterns": [
      {
        "type": "html",
        "pattern": "cookiebot|cookiebot\\.com",
        "description": "Cookiebot CMP",
        "vendor": "Cookiebot"
      },
      {
        "type": "html",
        "pattern": "onetrust|onetrust\\.com",
        "description": "OneTrust CMP",
        "vendor": "OneTrust"
      },
      {
        "type": "html",
        "pattern": "cookieconsent|cookieconsent\\.com",
        "description": "Cookie Consent CMP",
        "vendor": "Cookie Consent"
      },
      {
        "type": "html",
        "pattern": "efilli|efilli\\.com",
        "description": "Efilli CMP",
        "vendor": "Efilli"
      },
      {
        "type": "html",
        "pattern": "gdpr|cookie-law|cookie-policy",
        "description": "Generic GDPR/Cookie consent",
        "vendor": "Generic"
      },
      {
        "type": "html",
        "pattern": "accept-cookies|cookie-accept|consent-accept",
        "description": "Cookie acceptance buttons",
        "vendor": "Generic"
      },
      {
        "type": "javascript",
        "pattern": "__cmp|__tcfapi",
        "description": "CMP API functions",
        "vendor": "Generic CMP"
      },
      {
        "type": "javascript",
        "pattern": "googletagmanager\\.com|gtm\\.start",
        "description": "Google Tag Manager",
        "vendor": "Google Tag Manager"
      },
      {
        "type": "html",
        "pattern": "GTM-[A-Z0-9]+",
        "description": "GTM Container ID",
        "vendor": "Google Tag Manager"
      },
      {
        "type": "html",
        "pattern": "data-testid=[\"']cookie-consent[\"']",
        "description": "Cookie consent test IDs",
        "vendor": "Generic"
      }
    ],
    "selectors": [
      "[data-testid*='cookie']",
      "[id*='cookie']",
      "[class*='cookie']",
      "[data-cy*='consent']",
      "[aria-label*='cookie']",
      "button[onclick*='accept']",
      "a[href*='privacy']",
      ".gdpr-banner",
      ".cookie-banner",
      ".consent-banner",
      ".efilli-consent",
      ".efilli-banner",
      "[data-efilli]",
      "[class*='efilli']",
      "[data-gtm]",
      ".gtm-banner",
      ".gtm-consent",
      "[class*='gtm']"
    ]
  },
  "collectors": {
    "name": "Collector Patterns",
    "enabled": true,
    "patterns": [
      "forms.example.com/*",
      "yourbrand.formstack.com/*"
    ],
    "include_subdomains": false
  },
  "metadata": {
    "description": "Form detection, CMP patterns, and collector patterns configuration",
    "last_updated": "2024-12-19",
    "author": "form-scanner",
    "supported_forms": ["ozyegin", "yourcompany", "example", "google", "hubspot", "microsoft", "formstack"],
    "supported_cmp_vendors": ["Cookiebot", "OneTrust", "Cookie Consent", "Efilli", "Google Tag Manager", "Generic"],
    "supported_collectors": ["Custom glob patterns for link/iframe/script/form-action detection"]
  }
}
